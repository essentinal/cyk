<grammar>
  <rule>
    <left>
      <symbol>S</symbol>
    </left>
    <right>
      <symbol>A</symbol>
      <symbol>B</symbol>
    </right>
  </rule>
  <rule>
    <left>
      <symbol>S</symbol>
    </left>
    <right>
      <symbol>S</symbol>
      <symbol>S</symbol>
    </right>
  </rule>
  <rule>
    <left>
      <symbol>A</symbol>
    </left>
    <right>
      <symbol>0</symbol>
    </right>
  </rule>
  <rule>
    <left>
      <symbol>B</symbol>
    </left>
    <right>
      <symbol>1</symbol>
    </right>
  </rule>
</grammar>